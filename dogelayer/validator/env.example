#=====================================================================
# Bittensor variables
NETUID=109
SUBTENSOR_NETWORK=finney
BT_WALLET_NAME="validator1"
BT_WALLET_HOTKEY="default"


#=====================================================================
# Subnet Proxy Configuration (for regular validators)
# Contact DogeLayer team to obtain these credentials
SUBNET_PROXY_API_URL="https://api.dogelayer.ai"
SUBNET_PROXY_API_TOKEN="your-api-token-here"


#=====================================================================
# Subnet Owner Configuration (ONLY for subnet owner)
# ⚠️ WARNING: These parameters are ONLY for the subnet owner
# Regular validators should LEAVE THESE COMMENTED OUT
# Uncomment and set these ONLY if you are the subnet owner
#
# PROXY_DOMAIN="your-proxy-domain.com"
# PROXY_PORT="3331"
# PROXY_HIGH_DIFF_PORT="3332"
# PROXY_API_PORT="8888"
# PROXY_USERNAME="your-pool-username"
# PROXY_PASSWORD="x"
# PROXY_API_TOKEN="your-api-token"


#=====================================================================
# Optional: Database submission
SUBMIT_VALIDATOR_INFO=true
DB_SUBMIT_INTERVAL_SECONDS=300


#=====================================================================
# Recovery Configuration
RECOVERY_FILE_PATH=~/.bittensor/data/dogelayer/validator
RECOVERY_FILE_NAME=validator_state.json